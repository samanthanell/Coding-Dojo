// 4. Iterate through a given array, printing each value.
// PrintArrayVals(arr) DONE
function PrintArrayVals(arr) {
    for (let i = 0; i < arr.length; i++) {
        console.log(arr[i])
    }
}
PrintArrayVals([0, 1, 4, 7])

// 5. PrintMaxOfArray(arr)
// Given an array, find and print its largest element

function PrintMaxOfArray(array) {
    let x=0
    for (let i = 0; i < array.length; i++) {
        if (array[i] > x) {
    x=array[i]
        }
    }
    console.log(x)
}

PrintMaxOfArray([2, 1, 2])
        

        PrintMaxOfArray([0, 4, 2, 9])
