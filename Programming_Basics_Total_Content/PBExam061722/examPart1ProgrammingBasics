// 2. PrintOdds1To255() DONE
// Print all odd integers from 1 to 255.
function PrintOdds1To255() {
    for (let i = 1; i <=255; i++) {
        if (i % 2==1) {
            console.log(i)
        }
        }
        
    }
PrintOdds1To255()

